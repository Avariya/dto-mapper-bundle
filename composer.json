{
  "name": "vklymniuk/dto-mapper-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle, easy way to convert data.",
  "license": "MIT",
  "authors": [
    {
      "name": "Vladimir Klymniuk",
      "email": "v.klymniuk@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "indigophp/doctrine-annotation-autoload": "^0.1.0",
    "symfony/http-kernel": "^4.0",
    "symfony/dependency-injection": "^4.0",
    "symfony/config": "^4.0",
    "symfony/proxy-manager-bridge": "^4.0",
    "vklymniuk/dto-mapper": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "symfony/framework-bundle": "^4.0",
    "symfony/yaml": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "VKMapperBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": { "Tests\\": "tests/" }
  },
  "keywords": [
    "php",
    "mapper",
    "dto",
    "dto-mapper",
    "symfony",
    "serialization",
    "hydrator",
    "performance",
    "deserialization",
    "array conversion",
    "object conversion",
    "serializer",
    "deserialization"
  ],
  "scripts": {
    "test": "phpunit -c tests/phpunit.xml tests/",
    "post-install-cmd": [
      "composer dump-autoload"
    ]
  }
}
