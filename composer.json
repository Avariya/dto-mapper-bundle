{
  "name": "vklymniuk/dto-mapper-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle, easy way mapping data to DTO objects",
  "license": "MIT",
  "authors": [
    {
      "name": "Vladimir Klymniuk",
      "email": "v.klymniuk@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "indigophp/doctrine-annotation-autoload": "^0.1.0",
    "symfony/http-kernel": "^4.0",
    "symfony/dependency-injection": "^4.0",
    "symfony/config": "^4.0",
    "vklymniuk/dto-mapper": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2"
  },
  "autoload":     {
    "psr-4": { "DTOMapperBundle\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Tests\\": "tests/" }
  },
  "keywords": [
    "php",
    "mapper",
    "dto-mapper",
    "hydrator",
    "performance",
    "array conversion",
    "object conversion",
    "serializer"
  ],
  "scripts": {
    "test": "phpunit"
  }
}
